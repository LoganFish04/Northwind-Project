@{
}

<table class="table table-bordered">
    <tr>
        <th> Category Id </th>
        <th> Category Name </th>
        <th> Description </th>
    </tr>


    @foreach (var p in ViewBag.ListOfCategories)
    {
        <tr>
            <td> @p.CategoryId </td>
            <td> @p.CategoryName </td>
            <td> @p.Description </td>
        </tr>
    }

</table>